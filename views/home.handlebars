{{#with data}}
  <div class="position-relative overflow-hidden p-3 p-md-5 m-md-3 text-left">
        <div  class="col-md-10 p-lg-2 mx-auto my-4">
          <p class="home-intro">Hello, my name is Juan. I am a developer from Compton, CA. I enjoy building projects through every level of the implementation process.</p>
        </div>
        <div class="product-device shadow-sm d-none d-md-block"></div>
        <div class="product-device product-device-2 shadow-sm d-none d-md-block"></div>
      </div>
  </div>

    <main role="main">
          <div class="row">
            {{#each projects}}
            <div class="col-md-6">
                  <div class="container">
              <!-- Normal Demo-->
                <!-- Post-->
                <div class="post-module">
                  <!-- Thumbnail-->
                  <div class="thumbnail">
                  <img src={{img}}>
                  </div>
                  <!-- Post Content-->
                  <div class="post-content">
                    <a class='nav-item-fa' href={{url}}><h1 class="title" style="color:{{color}}">{{{title}}}</h1></a>
                    <!--
                    <h1 class="title" style="color:{{color}}">{{{title}}}</h1>
                    -->
                    <h2 class="sub_title">{{{description}}}</h2>
                    <p class="description">{{{info.[0].content}}}</p>
                    <div class="post-meta">
                      <span class="timestamp">
                         {{{info.[4].content}}}
                      </span>
                    </div>
                  </div>
                   <a href={{url}}></a>
              </div>
              </div>

          </div>


           {{/each}}
    </main>



 {{/with}}